<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Node</title>
    </head>
    <body>
        {{> header}}
        <h1>Images</h1>
        {{#each datas}}
        <p>{{this.title}}</p>
        <img src="data:image/jpeg;base64,{{bufferImage this.img.data}}" style="width: 200px;height: auto;display: block" alt="image">
        {{/each}}
    </body>
</html>